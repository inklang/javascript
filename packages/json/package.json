{
  "name": "@inklang/json",
  "version": "0.0.0",
  "description": "Provides support for the json namespace in inklang.",
  "author": "Mikkel ALMONTE--RINGAUD <contact@vexcited.com>",
  "license": "MIT",
  "repository": {
    "directory": "packages/json",
    "url": "git+https://github.com/inklang/javascript.git"
  },
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "terser": "^5.39.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  }
}
